    "@todo/tsconfig": "workspace:*",
    "@types/react": "19.1.17",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.2",
    "postcss": "^8.5.6",
    "prettier": "^3.8.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  },
  "prettier": "@todo/prettier-config"
}
