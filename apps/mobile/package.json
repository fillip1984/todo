{
  "name": "mobile",
  "private": true,
  "main": "index.ts",
  "scripts": {
    "build": "pnpm lint",
    "build:interactive": "pnpm lint && pnpm with-env eas build -p ios -e production --local",
    "build:production": "pnpm lint && pnpm with-env eas build -p ios -e production --local --non-interactive",
    "clean": "git clean -xdf .cache .expo  android ios node_modules",
    "ios": "expo run:ios",
    "format": "prettier --check . --ignore-path ../../.gitignore --ignore-path .prettierignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@better-auth/expo": "1.4.13",
    "@expo/ui": "0.2.0-beta.9",
    "@expo/vector-icons": "^15.0.3",
    "@legendapp/list": "^2.0.19",
    "@tanstack/react-query": "^5.90.18",
    "@trpc/client": "^11.8.1",
    "@trpc/server": "^11.8.1",
    "@trpc/tanstack-react-query": "^11.8.1",
    "better-auth": "1.4.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "expo": "~54.0.31",
    "expo-constants": "~18.0.13",
    "expo-dev-client": "~6.0.20",
    "expo-font": "^14.0.10",
    "expo-linking": "~8.0.11",
    "expo-network": "^8.0.8",
    "expo-router": "~6.0.21",
    "expo-secure-store": "~15.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "nativewind": "5.0.0-preview.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-native": "~0.81.5",
    "react-native-css": "3.0.1",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.6",
    "react-native-safe-area-context": "~5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-worklets": "~0.5.1",
    "superjson": "2.2.6",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@todo/api": "workspace:*",
