{
  "name": "@todo/db",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "default": "./src/client.ts"
    },
    "./schema": {
      "types": "./dist/schema/index.d.ts",
      "default": "./src/schema/index.ts"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "pnpm lint && tsc",
    "clean": "git clean -xdf .cache  dist node_modules",
    "dev": "tsc",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "push": "pnpm with-env drizzle-kit push",
    "push:prod": "pnpm dotenv -e ../../.env.prod -- drizzle-kit push",
    "studio": "pnpm with-env drizzle-kit studio",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "export": "pnpm with-env drizzle-kit export",
    "upgrade": "pnpm with-env drizzle-kit up",
    "relations": "pnpm with-env drizzle-kit pull",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^3.3.0",
    "drizzle-orm": "1.0.0-beta.15-859cf75",
    "pg": "^8.18.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@todo/eslint-config": "workspace:*",
    "@todo/prettier-config": "workspace:*",
    "@todo/tsconfig": "workspace:*",
    "@types/node": "25.2.1",
    "@types/pg": "^8.16.0",
    "drizzle-kit": "1.0.0-beta.15-859cf75",
    "eslint": "^9.39.2",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3"
  },
  "prettier": "@todo/prettier-config"
}
